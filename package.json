{
  "name": "nuxt-authentication",
  "version": "1.0.7",
  "description": "Nuxt Authentication is a simple module that proposes authentication functionalities for Nuxt applications that uses backends like Django REST framework or Laravel Sanctum.",
  "repository": "https://github.com/Zadigo/nuxt-authentication",
  "license": "MIT",
  "readme": "README.md",
  "author": {
    "name": "John Pendenque",
    "email": "pendenquejohn@gmail.com"
  },
  "keywords": [
    "nuxt",
    "authentication",
    "django",
    "login",
    "logout",
    "fetch"
  ],
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/types.d.mts",
      "import": "./dist/module.mjs"
    }
  },
  "main": "./dist/module.mjs",
  "typesVersions": {
    "*": {
      ".": [
        "./dist/types.d.mts"
      ]
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "prepack": "nuxt-module-build build",
    "dev": "npm run dev:prepare && nuxi dev playground",
    "dev:build": "nuxi build playground",
    "dev:prepare": "nuxt-module-build build --stub && nuxt-module-build prepare && nuxi prepare playground",
    "release": "npm run lint && npm run test && npm run prepack && changelogen --release && npm publish && git push --tags",
    "lint": "eslint .",
    "test": "vitest run",
    "test:watch": "vitest watch",
    "test:types": "vue-tsc --noEmit && cd playground && vue-tsc --noEmit"
  },
  "dependencies": {
    "@nuxt/kit": "^4.2.1",
    "@vueuse/core": "^14.0.0",
    "@vueuse/integrations": "^14.0.0",
    "defu": "^6.1.4",
    "jwt-decode": "^4.0.0",
    "ufo": "^1.6.1"
  },
  "devDependencies": {
    "@nuxt/devtools": "^3.1.0",
    "@nuxt/eslint": "1.10.0",
    "@nuxt/eslint-config": "^1.10.0",
    "@nuxt/module-builder": "^1.0.2",
    "@nuxt/schema": "^4.2.1",
    "@nuxt/scripts": "0.13.0",
    "@nuxt/test-utils": "^3.20.1",
    "@nuxt/ui": "4.1.0",
    "@tailwindcss/vite": "^4.1.17",
    "@types/node": "latest",
    "@unhead/vue": "^2.0.19",
    "@vueuse/nuxt": "^14.0.0",
    "changelogen": "^0.6.2",
    "eslint": "^9.39.1",
    "nuxt": "^4.2.1",
    "tailwindcss": "^4.1.17",
    "typescript": "~5.9.3",
    "vitest": "^4.0.8",
    "vue-tsc": "^3.1.3"
  }
}
